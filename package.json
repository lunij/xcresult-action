{
  "name": "xcresult",
  "version": "1.0.0",
  "private": false,
  "description": "Generates GitHub Markdown from an Xcode result bundle and displays it as a GitHub Check.",
  "main": "dist/main.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write '**/*.ts'",
    "format-check": "prettier --check '**/*.ts'",
    "lint": "eslint",
    "package": "ncc build src/main.ts --source-map --license licenses.txt",
    "pretest": "npm run build",
    "test": "jest",
    "all": "npm run build && npm run format && npm run lint && npm run package && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lunij/xcresult.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "Lunij",
  "license": "MIT",
  "dependencies": {
    "@actions/artifact": "2.1.9",
    "@actions/core": "1.10.1",
    "@actions/exec": "1.1.1",
    "@actions/github": "6.0.0",
    "@octokit/action": "7.0.0",
    "archiver": "^7.0.1",
    "image-size": "1.1.1"
  },
  "devDependencies": {
    "@stylistic/eslint-plugin-js": "^2.8.0",
    "@stylistic/eslint-plugin-ts": "^2.9.0",
    "@types/archiver": "^6.0.2",
    "@types/node": "^22.6.1",
    "@vercel/ncc": "^0.38.2",
    "eslint": "^9.11.1",
    "eslint-plugin-jest": "^28.0.0",
    "jest": "^29.5.0",
    "js-yaml": "^4.1.0",
    "prettier": "^3.0.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "5.5.2",
    "typescript-eslint": "^8.7.0"
  }
}
